@semantql/storage/
├── package.json
├── index.js              # Main exports
├── providers/
│   ├── index.js         # Provider exports
│   ├── uploadthing.js   # UploadThing provider
│   ├── s3.js           # AWS S3 provider
│   └── cloudinary.js    # Cloudinary provider -can add more
├── middleware.js        # Express middleware (single file)
├── utils.js            # All utils in one file
├── lib/
│   ├── StorageService.js       # Core storage service
│   ├── config.js              # Configuration loader (NEW)
│   └── ModelFileService.js    # Model-specific file handler (NEW)
├── templates/
│   └── storage-integrated-service.js  # Template for CLI (NEW)
└── README.md